<controls:TabViewItem
	x:Class="LRReader.Views.Tabs.SettingsTab"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:content="using:LRReader.Views.Tabs.Content"
	xmlns:controls="using:Microsoft.UI.Xaml.Controls"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:local="using:LRReader.Views.Tabs"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	d:DesignHeight="300"
	d:DesignWidth="400"
	Header="Settings"
	mc:Ignorable="d">
	<controls:TabViewItem.IconSource>
		<controls:SymbolIconSource Symbol="Setting" />
	</controls:TabViewItem.IconSource>

	<content:SettingsTabContent x:Name="TabContent" />
</controls:TabViewItem>
