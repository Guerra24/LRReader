<ContentDialog
	x:Class="LRReader.UWP.Views.Dialogs.CreateTankoubon"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:local="using:LRReader.UWP.Views.Dialogs"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	x:Uid="/Dialogs/CreateTankoubon/Dialog"
	IsPrimaryButtonEnabled="False"
	Style="{StaticResource DefaultContentDialogStyle}"
	mc:Ignorable="d">

	<StackPanel Spacing="10">
		<TextBox x:Name="TankoubonName" x:Uid="/Dialogs/CreateTankoubon/Name" TextChanging="Name_TextChanging" />
		<TextBlock Name="Error" Foreground="#FF4141" />
	</StackPanel>
</ContentDialog>
